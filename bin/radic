#!/usr/bin/env php
<?php

require __DIR__.'/../vendor/autoload.php';
require __DIR__.'/../boot.php';

use Symfony\Component\Console\Application;

$application = new Application('RadicGitter', '0.0.1');
foreach(radic()->files()->listContents('src/commands') as $file)
{
    radic()->dump($file);
}



$application->run();
